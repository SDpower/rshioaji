# Shioaji API Configuration
# Copy this file to .env and fill in your actual values

# API credentials (required)
SHIOAJI_API_KEY=your_api_key_here
SHIOAJI_SECRET_KEY=your_secret_key_here

# Trading mode (optional, defaults to true)
SHIOAJI_SIMULATION=true

# Alternative environment variable names (also supported)
# API_KEY=your_api_key_here
# SECRET_KEY=your_secret_key_here
# SIMULATION=true

# Logging level (optional)
RUST_LOG=info

# Example values:
# SHIOAJI_API_KEY=1234567890abcdef
# SHIOAJI_SECRET_KEY=abcdef1234567890
# SHIOAJI_SIMULATION=false